{
    "name": "design-token-test",
    "displayName": "design-token-test",
    "description": "jsx highlighting for design tokens",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.45.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "grammars": [{
            "language": "jsx-attr",
            "scopeName": "source.jsx",
            "injectTo": ["source.js"],
            "path": "./grammars/design-token.json",
            "embeddedLanguages": {
                "meta.tag.jsx": "jsx",
                "meta.jsx.tag.termination": "jsx",
                "JSXNested": "jsx",
                "JSXAttrs": "jsx-attr",
                "source.inside-js.css.styled": "styled-css"
            }
        }]
    }
}