{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "test-design.injection",
	"fileTypes": ["js", "jsx", "ts", "tsx"],
	"injectionSelector": "L:string.quoted.double.js",
	"patterns": [
		{
			"include": "#strings"
		}

	],
	"repository": {
		"strings": {
			"patterns": [
				{
					"name": "keyword.functionalGreenLight",
					"match": "functional.greenLight"
				}
			]
		}
	},
	"scopeName": "source.jsx"
}